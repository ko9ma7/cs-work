public class p2{
	public static void main(String[] args){
		int num = 0;
		
		//add code to get num from command line argument
		//call showPrimes
		//showPrimes( num );
	}

	public static void showPrimes(int N){
		boolean primeCheck[] = new boolean[N+1];
		primeCheck[0] = false;
		primeCheck[1] = false;
		
		for (int x=2; x<(primeCheck.length); x++){
			primeCheck[x] = true;
		}
			
		for (int i=x; i<=( Math.sqrt(N) ); i++){
			if (primeCheck[i]) {
				for (int j=(i*i); j<=N; j++){
					primeCheck[j] = false;						
				}
			}
		}		
	} 
}